<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                             http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

    <display-name>AdministradorArchivosWebApp</display-name>

    <servlet>
        <servlet-name>AdminArchivos</servlet-name>
        <servlet-class>me.jmll.AdminArchivos</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AdminArchivos</servlet-name>
        <url-pattern>/Admin.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ListaArchivos</servlet-name>
        <servlet-class>me.jmll.ListaArchivos</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ListaArchivos</servlet-name>
        <url-pattern>/ListaArchivos</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DescargaArchivo</servlet-name>
        <servlet-class>me.jmll.DescargaArchivo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DescargaArchivo</servlet-name>
        <url-pattern>/DescargaArchivo</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>

</web-app>